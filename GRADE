CREATE PROCEDURE MARK3 (MARK INT)
 BEGIN
 IF (MARK >=50) THEN
 SELECT ('A') AS GRADE ;
 ELSEIF (MARK >=40) AND (MARK<50) THEN
 SELECT ('B')AS GRADE;
 ELSEIF (MARK >=30) AND (MARK<40) THEN
 SELECT ('C') AS GRADE;
 ELSEIF (MARK >=20) AND (MARK<30) THEN
 SELECT ('D') AS GRADE;
 ELSE
 SELECT ('F') AS GRADE;
 END IF;
 END; 
